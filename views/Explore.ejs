<style>
h1 {
  font-size: 62.5%;
  font-family: arial, sans-serif;
}


/* Always take up entire screen */
#page-content-wrapper {
    width: 100%;
    position: absolute;
    padding-top: 50px;
    border: 5px;
}

.col-centered{
    float: none;
    margin: 0 auto;
}
.jumbotron1{
  background-color:rgba(255,255,255,0);
  border-radius: 25px;
  position:fixed;
  top:100px;
  left:100px;
  z-index=1;
  text-align: center;

}

.slide1{
  background-image: url('/images/explore/band1.jpg');
  width:100%;
  height:760px;
  background-repeat: no-repeat;
  background-position:center;
  background-size:cover;
  opacity: 0.78;
}
.slide2{
  background-image: url('/images/explore/beach.jpg');
  width:100%;
  height:760px;
  background-repeat: no-repeat;
  background-position:center;
  background-size:cover;
  opacity:0.78;
}
.slide3{
  background-image: url('/images/explore/band2.jpg');
  width:100%;
  height:760px;
  background-repeat: no-repeat;
  background-position:center;
  background-size:cover;
  opacity:0.78;
}
.carousel-caption h1{
  font-size: 5.4em;
  font-family: 'Pacifico', cursive;
  padding-bottom: .7em;
}
.cl-text{
  z-index=1;
}

.carousel-caption p{
  font-size: 2em;
}


body{
    font-family: "Montserrat", "Helvetica Neue", Helvetica, Arial, sans-serif;
}


.btn{
      background-color: white; 
    color: black; 
    border: 2px solid MediumAquaMarine;
}

.btn:hover {
    background-color: MediumAquaMarine;
    color: white;
}

.btn-past{
    background-color: MediumAquaMarine;
    color: white;
    border: 2px solid MediumAquaMarine;
}

.btn-past:hover{
    background-color: white; 
    color: black; 
    border: 2px solid MediumAquaMarine;
}

.intro-text {
    text-transform: uppercase;
    font-size: 2em;
    font-weight: 400;
    letter-spacing: 1px;
}


footer {
  padding: 25px 0;
  text-align: center;
  width:100%;
}

footer span.copyright {
  line-height: 40px;
  font-family: "Montserrat", "Helvetica Neue", Helvetica, Arial, sans-serif;
  text-transform: uppercase;
  text-transform: none;
}

footer ul.quicklinks {
  margin-bottom: 0;
  line-height: 40px;
  font-family: "Montserrat", "Helvetica Neue", Helvetica, Arial, sans-serif;
  text-transform: uppercase;
  text-transform: none;
}


ul.list-inline {
  margin-bottom: 30px;
  margin-top: 0;
}

ul.list-inline li a{
      color:orange;
}

ul.social-buttons {
  margin-bottom: 0;
}
ul.social-buttons li a {
  display: block;
  background-color: #222222;
  height: 40px;
  width: 40px;
  border-radius: 100%;
  font-size: 20px;
  line-height: 40px;
  color: white;
  outline: none;
  -webkit-transition: all 0.3s;
  -moz-transition: all 0.3s;
  transition: all 0.3s;
}
ul.social-buttons li a:hover,
ul.social-buttons li a:focus,
ul.social-buttons li a:active {
  background-color: #fed136;
}

textarea{
  resize: none;
  height:132px !important;
}

</style>

<body>


 <%- include navBar.ejs -%>

 <div id="wrapper">

  <div id="page-content-wrapper">

    <div id="theCarousel" class="carousel slide" data-ride="carousel" >
      <ol class="carousel-indicators">
        <li data-target="#theCarousel" data-silde-to="0" class="active"></li>
        <li data-target="#theCarousel" data-silde-to="1" ></li>
        <li data-target="#theCarousel" data-silde-to="2" ></li>
      </ol>
      <div class="carousel-inner">

        <div class="item active">
          <div class="slide1"></div>
          <div class="carousel-caption">
            <h1 class="cl-text" style="color: saddlebrown">Whatever  you <br>want</h1>
            <p style="color:black;">Search by keywords</p>
            <form >
              <i class="fa fa-search fa-lg"></i><input type="text" id="search_text" name="SearchItem" value="" placeholder="Keywords" style="opacity:0.7;color:black;"></input>
              <br><br><a class="btn" id="search_button" style="padding: 8px 25px">Search</a>
            </form>
            <br>
            <br>
            <br>
          </div>
        </div>
        <div class="item">
          <div class="slide2"></div>
          <div class="carousel-caption">
            <h1 class="cl-text" style="color: chocolate;padding-bottom: 1em !important">Wherever you <br>are</h1>
            <p style="color:black;padding-bottom: 0.5em">View by location</p>
            <form >
              <i class="fa fa-search fa-lg"></i><input type="text" id="search_location" name="SearchItem" value="" placeholder="Keywords" style="opacity:0.7;color:black;"></input>
              <br><br><a class="btn" id="map_button" style="padding: 8px 25px">Map</a>
            </form>
            <br>
            <br>
          </div>
        </div>
        <div class="item">
          <div class="slide3"></div>
          <div class="carousel-caption">
            <h1 class="cl-text" style="color: orange; padding-bottom: 1.1em !important">We know you <br>best </h1>
            <p style="color:black;padding-bottom: 1.2em !important">Enjoy our recommendation</p>
            <p><a href="#" class="btn" style="padding: 8px 25px">Check</a></p>
            <br>
            <br>
          </div>
        </div>

      </div>
      <a class="left carousel-control" href="#theCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
      </a>
      <a class="right carousel-control" href="#theCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
      </a>
    </div>

    <!--contact section-->
    <br><br>
    <section id="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Cannot find what you want?</h2>
                    <h3 class="section-subheading text-muted">Let us know!</h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    <form name="sentMessage" id="contactForm" novalidate>
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <input type="text" class="form-control" placeholder="Your Name *" id="name" required data-validation-required-message="Please enter your name.">
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
                                    <input type="email" class="form-control" placeholder="Your Email *" id="email" required data-validation-required-message="Please enter your email address.">
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
                                    <input type="tel" class="form-control" placeholder="Your Phone *" id="phone" required data-validation-required-message="Please enter your phone number.">
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <textarea class="form-control" placeholder="Your Message *" id="message" required data-validation-required-message="Please enter a message."></textarea>
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                            <div class="col-lg-12 text-center">
                                <div id="success"></div>
                                <button type="submit" class="btn btn-xl">Send Message</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </section>

    <hr>

            <%- include footer.ejs -%>
  </div>  
</div>


</body>

</html>
